<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="listBean" class="ioc.collection.CollectionBean">
	<property name="list">
		<list>
			<value>윤영현</value>
			<value>김기동</value>
			<value>황석호</value>
			<value>배현서</value>
			<value>최솔희</value>
		</list>
	</property>
</bean>

<bean id="setBean" class="ioc.collection.CollectionBean">
	<property name="set">
		<set>
			<value>윤영현</value>
			<value>김기동</value>
			<value>황석호</value>
			<value>배현서</value>
			<value>최솔희</value>
		</set>
	</property>
</bean>

<bean id="mapBean" class="ioc.collection.CollectionBean">
	<property name="map">
		<map>
			<entry>
				<key><value>윤영현</value></key>
				<value>100</value>
			</entry>
			<entry>
				<key><value>김기동</value></key>
				<value>86</value>
			</entry>
			<entry>
				<key><value>황석호</value></key>
				<value>92</value>
			</entry>
			<entry>
				<key><value>배현서</value></key>
				<value>91</value>
			</entry>
			<entry>
				<key><value>최솔희</value></key>
				<value>88</value>
			</entry>
		</map>
	</property>
</bean>

<bean id="propBean" class="ioc.collection.CollectionBean">
	<property name="prop">
		<props>
			<prop key="윤영현">100</prop>
			<prop key="김기동">86</prop>
			<prop key="황석호">92</prop>
			<prop key="배현서">91</prop>
			<prop key="최솔희">88</prop>
		</props>
	</property>
</bean>
</beans>
